{
  "name": "@aidenlx/obsidian-icon-shortcodes",
  "version": "0.9.6",
  "description": "API utils for obsidian-icon-shortcodes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "type": "module",
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "prepare": "node ./scripts/generate-icon.js",
    "dev": "cross-env BUILD=dev node esbuild.config.mjs",
    "check": "tsc --noEmit",
    "prelib": "rm -rf lib && node ./scripts/cp-dts.mjs",
    "lib": "tsc --project tsconfig-lib.json",
    "build": "pnpm run lib && cross-env BUILD=production node esbuild.config.mjs",
    "build-beta": "cross-env BUILD=production BETA=1 node esbuild.config.mjs",
    "prettier": "prettier --write 'src/**/*.+(ts|tsx|json|html|css)'",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "release-ob": "release-it --config .release-it.ob.cjs",
    "release-full": "release-it",
    "release-lib": "release-it --config .release-it.lib.cjs",
    "release-beta": "release-it --config .release-it.beta.cjs"
  },
  "keywords": [],
  "author": "AidenLx",
  "license": "MIT",
